{
    "private": true,
    "scripts": {
        "dev": "$npm_execpath run development --scripts-prepend-node-path",
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js --scripts-prepend-node-path",
        "watch": "$npm_execpath run development -- --watch --scripts-prepend-node-path",
        "watch-poll": "$npm_execpath run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js --scripts-prepend-node-path",
        "prod": "$npm_execpath run production --scripts-prepend-node-path",
        "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js --scripts-prepend-node-path",
        "svg": "vsvg -s ./nova/resources/svg/feather -t ./nova/resources/js/components/icons/icons-feather --svgo=./svgo.js"
    },
    "dependencies": {
        "@babel/plugin-syntax-dynamic-import": "^7.2.0",
        "@inertiajs/inertia": "^0.1.3",
        "@inertiajs/inertia-vue": "^0.1.1",
        "animate.css": "^3.7",
        "axios": "^0.18",
        "buefy": "^0.7.3",
        "feather-icons": "^4.10",
        "laravel-mix": "^4.0.7",
        "laravel-mix-tailwind": "^0.1.0",
        "lodash": "^4.17",
        "mousetrap": "^1.6.3",
        "popper.js": "^1.12",
        "portal-vue": "^2.1.0",
        "resolve-url-loader": "^2.3.1",
        "slug": "^0.9.3",
        "tailwindcss-dark-mode": "^1.0.3",
        "tailwindcss": "^1.1.0",
        "vue": "^2.6.0",
        "vue-feather-icons": "^4.7",
        "vue-on-click-outside": "^1.0.3",
        "vue-tippy": "^2.1.2",
        "vuex": "^3.1.0",
        "vuex-pathify": "^1.2.1"
    },
    "devDependencies": {
        "cross-env": "^5.1",
        "eslint": "^5.5.0",
        "eslint-config-airbnb-base": "^13.1.0",
        "eslint-import-resolver-webpack": "^0.10.1",
        "eslint-plugin-import": "^2.14.0",
        "eslint-plugin-vue": "^5.2.0",
        "postcss-import": "^12.0.1",
        "postcss-nested": "^4.1.2",
        "tailwind-css-variables": "^2.0.2",
        "vue-class-component": "^7.0.2",
        "vue-template-compiler": "^2.6.0"
    }
}
