{
    "private": true,
    "scripts": {
        "dev": "npm run development --scripts-prepend-node-path",
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js --scripts-prepend-node-path",
        "watch": "npm run development -- --watch --scripts-prepend-node-path",
        "watch-poll": "npm run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js --scripts-prepend-node-path",
        "prod": "npm run production --scripts-prepend-node-path",
        "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js --scripts-prepend-node-path",
        "svg": "vsvg -s ./nova/resources/svg/feather -t ./nova/resources/js/components/icons/icons-feather --svgo=./svgo.js"
    },
