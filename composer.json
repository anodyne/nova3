{
    "name": "anodyne/nova3",
    "type": "project",
    "description": "Nova 3 RPG Management",
    "keywords": [
        "cms",
        "anodyne",
        "nova",
        "rpg"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "anodyne/blade-tabler-icons": "^3.0",
        "artesaos/seotools": "^1.3",
        "awcodes/filament-tiptap-editor": "^3.0",
        "awcodes/scribble": "^0.3.0@alpha",
        "aws/aws-sdk-php": "^3.293",
        "bensampo/laravel-embed": "^2.4",
        "blade-ui-kit/blade-icons": "^1.0",
        "filament/forms": "^3.0-stable",
        "filament/notifications": "^3.0-stable",
        "filament/spatie-laravel-media-library-plugin": "^3.2",
        "filament/support": "^3.0-stable",
        "filament/tables": "^3.0-stable",
        "guzzlehttp/guzzle": "^7.2",
        "lab404/laravel-impersonate": "^1.7",
        "laravel/fortify": "^1.16",
        "laravel/framework": "^11.0",
        "laravel/pulse": "^1.2",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.7",
        "livewire/livewire": "^3.4",
        "lorisleiva/laravel-actions": "^2.1",
        "mailersend/laravel-driver": "^2.3",
        "santigarcor/laratrust": "^8.0",
        "spatie/eloquent-sortable": "^4.0",
        "spatie/laravel-activitylog": "^4.0",
        "spatie/laravel-collection-macros": "^7.1",
        "spatie/laravel-data": "^4.2",
        "spatie/laravel-honeypot": "^4.5",
        "spatie/laravel-html": "^3.0",
        "spatie/laravel-medialibrary": "^11.0",
        "spatie/laravel-model-states": "^2.1",
        "spatie/laravel-ray": "^1.26",
        "staudenmeir/eloquent-has-many-deep": "^1.18",
        "staudenmeir/laravel-adjacency-list": "^1.0",
        "symfony/http-client": "^7.0",
        "symfony/mailgun-mailer": "^7.0",
        "symfony/postmark-mailer": "^7.0",
        "wire-elements/modal": "^2.0",
        "wire-elements/spotlight": "^2.0"
    },
    "require-dev": {
        "brianium/paratest": "^7.0",
        "fakerphp/faker": "^1.14",
        "itsgoingd/clockwork": "^5.0",
        "laravel/pint": "^1.8",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^8.1",
        "pestphp/pest": "^2.9",
        "pestphp/pest-plugin-laravel": "^2.1",
        "pestphp/pest-plugin-livewire": "^2.1",
        "phpunit/phpunit": "^10.0"
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "blade-ui-kit/blade-icons",
                "livewire-ui/spotlight"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Nova\\": "nova/src/",
            "Nova\\Foundation\\": "nova/foundation/",
            "Nova\\Setup\\": "nova/setup/",
            "Extensions\\": "extensions/",
            "Themes\\": "themes/",
            "Database\\Factories\\": "nova/database/factories/",
            "Database\\Seeders\\": "nova/database/seeders/"
        },
        "files": [
            "nova/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "analyse": "./vendor/bin/phpstan analyse",
        "clean": [
            "rm -f ./nova/bootstrap/cache/blade-icons.php",
            "rm -f ./nova/bootstrap/cache/packages.php",
            "rm -f ./nova/bootstrap/cache/services.php"
        ],
        "lint": "./vendor/bin/pint",
        "test": "./vendor/bin/pest",
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan optimize:clear --ansi",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan filament:upgrade"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
