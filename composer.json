{
    "name": "anodyne/nova3",
    "type": "project",
    "description": "Nova 3 RPG Management",
    "keywords": [
        "cms",
        "anodyne",
        "nova",
        "rpg"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "anodyne/blade-fluentui-icons": "^1.1",
        "anodyne/blade-tabler-icons": "^2.18",
        "awssat/discord-notification-channel": "^1.4",
        "blade-ui-kit/blade-icons": "^1.0",
        "filament/forms": "3.0.0-alpha104",
        "filament/notifications": "3.0.0-alpha104",
        "filament/support": "3.0.0-alpha104",
        "filament/tables": "3.0.0-alpha104",
        "guzzlehttp/guzzle": "^7.2",
        "kirschbaum-development/livewire-filters": "^0.5",
        "laravel/fortify": "^1.16",
        "laravel/framework": "^10.0",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.7",
        "livewire/livewire": "^2.12",
        "lorisleiva/laravel-actions": "^2.1",
        "saade/blade-iconsax": "^1.1",
        "santigarcor/laratrust": "^8.0",
        "spatie/eloquent-sortable": "^4.0",
        "spatie/laravel-activitylog": "^4.0",
        "spatie/laravel-collection-macros": "^7.1",
        "spatie/laravel-data": "^3.5",
        "spatie/laravel-html": "^3.0",
        "spatie/laravel-livewire-wizard": "^1.0",
        "spatie/laravel-medialibrary": "^10.0",
        "spatie/laravel-model-states": "^2.1",
        "spatie/laravel-ray": "^1.26",
        "spatie/once": "^3.0",
        "staudenmeir/eloquent-eager-limit": "^1.6",
        "staudenmeir/laravel-adjacency-list": "^1.0",
        "wire-elements/modal": "^1.0",
        "wire-elements/spotlight": "^1.0"
    },
    "require-dev": {
        "brianium/paratest": "^7.0",
        "fakerphp/faker": "^1.14",
        "itsgoingd/clockwork": "^5.0",
        "jasonmccreary/laravel-test-assertions": "^2.0",
        "laravel/pint": "^1.8",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^7.0",
        "nunomaduro/phpinsights": "^2.0",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "blade-ui-kit/blade-icons",
                "livewire/livewire",
                "livewire-ui/spotlight"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Nova\\": "nova/src/",
            "Nova\\Foundation\\": "nova/foundation/",
            "Nova\\Setup\\": "nova/setup/",
            "Extensions\\": "extensions/",
            "Themes\\": "themes/",
            "Database\\Factories\\": "nova/database/factories/",
            "Database\\Seeders\\": "nova/database/seeders/"
        },
        "files": [
            "nova/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "nova/tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php -r \"file_exists('nova/vendor') && !file_exists('vendor') && symlink('nova/vendor', 'vendor');\"",
            "@php artisan optimize:clear --ansi",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan filament:upgrade"
        ]
    },
    "config": {
        "bin-dir": "nova/vendor/bin",
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "vendor-dir": "nova/vendor",
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}