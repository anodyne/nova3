{
    "name": "anodyne/nova3",
    "type": "project",
    "description": "Nova 3 RPG Management",
    "keywords": [
        "cms",
        "anodyne",
        "nova",
        "rpg"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "awssat/discord-notification-channel": "^1.4",
        "blade-ui-kit/blade-icons": "^1.0",
        "guzzlehttp/guzzle": "^7.2",
        "jessarcher/laravel-castable-data-transfer-object": "^2.0",
        "kalnoy/nestedset": "^6.0",
        "kirschbaum-development/livewire-filters": "^0.3",
        "laravel/framework": "^9.0",
        "laravel/sanctum": "^2.14.1",
        "laravel/tinker": "^2.7",
        "laravel/ui": "^3.4",
        "livewire/livewire": "^2.5.1",
        "lorisleiva/laravel-actions": "^2.1",
        "santigarcor/laratrust": "^7.0",
        "spatie/data-transfer-object": "^3.0",
        "spatie/laravel-activitylog": "^4.0",
        "spatie/laravel-collection-macros": "^7.1",
        "spatie/laravel-data": "^1.0",
        "spatie/laravel-html": "^3.0",
        "spatie/laravel-livewire-wizard": "^1.0",
        "spatie/laravel-medialibrary": "^10.0",
        "spatie/laravel-model-states": "^2.1",
        "spatie/laravel-ray": "^1.26",
        "spatie/once": "^3.0",
        "staudenmeir/eloquent-eager-limit": "^1.6",
        "wire-elements/modal": "^1.0",
        "wire-elements/spotlight": "^1.0"
    },
    "require-dev": {
        "brianium/paratest": "^6.1",
        "fakerphp/faker": "^1.14",
        "friendsofphp/php-cs-fixer": "^3.0",
        "itsgoingd/clockwork": "^5.0",
        "jasonmccreary/laravel-test-assertions": "^2.0",
        "laravel/sail": "^1.5",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^6.1",
        "nunomaduro/phpinsights": "^2.0",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "blade-ui-kit/blade-icons",
                "kalnoy/nestedset",
                "livewire/livewire",
                "livewire-ui/spotlight"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Nova\\": "nova/src/",
            "Nova\\Foundation\\": "nova/foundation/",
            "Nova\\Setup\\": "nova/setup/",
            "Extensions\\": "extensions/",
            "Themes\\": "themes/",
            "Database\\Factories\\": "nova/database/factories/",
            "Database\\Seeders\\": "nova/database/seeders/"
        },
        "files": [
            "nova/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "nova/tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php -r \"file_exists('nova/vendor') && !file_exists('vendor') && symlink('nova/vendor', 'vendor');\"",
            "@php artisan optimize:clear --ansi",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "config": {
        "bin-dir": "nova/vendor/bin",
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "vendor-dir": "nova/vendor",
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
